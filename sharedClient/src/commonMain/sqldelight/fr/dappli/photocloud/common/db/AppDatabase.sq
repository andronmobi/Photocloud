CREATE TABLE Cache (
    key TEXT NOT NULL UNIQUE,
    value TEXT NOT NULL
);

insertToCache:
INSERT INTO Cache(key, value)
VALUES(?, ?);

selectFromCacheByKey:
SELECT * FROM Cache
WHERE key = ?;

deleteAllCache:
DELETE FROM Cache;

deleteFromCacheByKey:
DELETE FROM Cache
WHERE key = ?;

update:
UPDATE Cache
SET value = ?
WHERE key = ?;
